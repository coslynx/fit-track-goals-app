{
  "name": "fitness-tracker",
  "version": "1.0.0",
  "description": "A web application for tracking fitness goals.",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server.js",
    "client:dev": "npm run start --prefix src",
    "build": "npm run build --prefix src"
  },
  "dependencies": {
    "jsonwebtoken": "9.0.2",
    "bcrypt": "5.1.1",
    "axios": "1.7.9",
    "react-router-dom": "7.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": "18.16.0",
    "npm": "9.5.1"
  }
}